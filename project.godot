[application]

config/name="Mocra: Online card collection"
run/main_scene="res://scenes/Main.tscn"
boot_splash/image="res://images/logos/launch_logo.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

Global="*res://Global.gd"
Networking="*res://Networking.gd"
TAD="*res://scripts/TAD.gd"
JsonParser="*res://JsonParser.gd"
MapSaver="*res://mocraAdventure/map/map_saver.gd"
Options="*res://Options.gd"
Queue_datatype="*res://scripts/strucured_types/queue.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/fullscreen=true
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="expand"

[global]

export=false
keep=false

